<h1>ユーザー一覧</h1><br>

<% @users.each do |user| %>
<tr>
  <th><%= user.id %></th>
  <td><%= link_to admin_user_path(user.id) do %>
      <%= user.name %>
      <% end %>
  </td>
  <td><%= user.email %></td>
  <td>
    <% if user.is_deleted == false %>
    <!--falseである場合-->
    <p class="text-success">有効</p>
    <% else %>
    <!--trueである場合-->
    <p class="text-secondary">退会</p>
    <% end %>
<% end %>